<%- include("partials/header"); -%>
  <h1>Home</h1>
  <p>
    <%= startingContent %>
  </p>


  <div class="blog-container">
    <% posts.forEach(function(post){ %>

      <h3>
        <%=post.title%>
      </h3>
      <p>
        <%=post.content.substring(0, 10) + " ..." %>
          <a href="/posts/<%=post._id%>">Read More</a>
      </p>
  
  
      <% }) %>
  </div>


      <%- include("partials/footer"); -%>